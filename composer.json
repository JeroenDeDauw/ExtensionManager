{
	"name": "mwjames/composer-packages",
	"type": "mediawiki-extension",
	"description": "List composer packages used within MedaiWiki",
	"keywords": [
		"composer"
	],
	"homepage": "https://github.com/mwjames/composer-packages",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "mwjames",
			"homepage": "https://github.com/mwjames"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"mwjames/service-registry": "~0.1"
	},
  "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/mwjames/service-registry"
        }
    ],
	"autoload": {
		"files" : [
			"ComposerPackages.php"
		],
		"psr-0": {
			"ComposerPackages\\": "src/"
		}
	}
}